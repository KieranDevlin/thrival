type User {
  id: ID! @id
  name: String!
  password: String!
}
type JobPost {
  id: ID! @id
  createdAt: DateTime! @createdAt
  industry: String!
  location: String!
  discipline: [String!]! @scalarList(strategy:RELATION)
  totalRoles: Int! 
}
type Employer {
  id: ID! @id
}
type Contact {
  id: ID! @id
  email: String
  website: String 
}
type Applicant {
  id: ID! @id
  email: String! @unique
  linkedin: String!
  github: String!
}